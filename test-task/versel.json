{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/api/cocktail/(.*)",
        "dest": "https://www.thecocktaildb.com/api/json/v1/1/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }